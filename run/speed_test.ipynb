    "# speed test on institute machine (beehive11) and private computer\n",
    "import time as t\n",
    "import numpy as np\n",
    "\n",
    "var = 0\n",
    "\n",
    "\n",
    "t0 = t.time()\n",
    "\n",
    "for x in range(int(1e6)):\n",
    "    var += np.sin(0.01*x)\n",
    "    \n",
    "tot_time = t.time() - t0\n",
    "print(\"Total time used: %.3f s\" % tot_time)\n",
    "\n",
    "\n",
    "\n",
    "\"\"\"\n",
    "Time usage\n",
